<configuration>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%date{HH:mm:ss.SSS}:
				%-15.15thread|%15.15logger{30}:%-4L|%-30.30method|%msg%n</pattern>
		</encoder>
	</appender>
	<root level="off">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>